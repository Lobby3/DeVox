{
  "name": "evm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "graph:init": "npx hardhat graph init",
    "test": "npx hardhat test --network hardhat",
    "build": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "coverage": "npx hardhat coverage",
    "deploy": "npx hardhat deploy",
    "docgen": "npx hardhat docgen",
    "hardhat-local": "hardhat node --hostname 0.0.0.0",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yml --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier": "prettier --config ../.prettierrc --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:check": "prettier --check --config ../.prettierrc \"**/*.{js,json,md,sol,ts,yml}\"",
    "graph-test": "graph test",
    "graph-codegen": "cd ./subgraph && graph codegen",
    "graph-build": "cd ./subgraph && graph build",
    "graph-local": "docker-compose up",
    "graph-local-clean": "docker-compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "create-local": "graph create --node http://127.0.0.1:8020 evm",
    "deploy-local": "cd ./subgraph && graph deploy --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020 evm"
  },
  "private": true,
  "dependencies": {
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@daohaus/baal-contracts": "^1.2.4",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@gnosis.pm/zodiac": "^3.3.0",
    "@graphprotocol/graph-cli": "^0.49.0",
    "@graphprotocol/graph-ts": "^0.30.0",
    "@graphprotocol/hardhat-graph": "https://github.com/graphprotocol/hardhat-graph",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@openzeppelin/contracts": "^4.9.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.0",
    "@openzeppelin/hardhat-upgrades": "^1.25.0",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.28",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.2.0",
    "solidity-coverage": "^0.8.2"
  }
}